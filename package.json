{
  "name": "svg-board",
  "scripts": {
    "start": "npm run parcel",
    "parcel": "parcel index.html",
    "build": "parcel build index.html",
    "format": "prettier --write '**/*.{js,ts,html,css,md}' --list-different",
    "lint": "eslint . --ext .ts",
    "clean": "rm -rf dist/ .parcel-cache/",
    "tsc": "tsc",
    "test": "tap --ts rect.test.ts",
    "download-font": "mkdir -p fonts && curl -o 'fonts/iosevka-regular.woff2' https://cdnjs.cloudflare.com/ajax/libs/Iosevka/6.0.0/iosevka/woff2/iosevka-regular.woff2",
    "postinstall": "npm run download-font"
  },
  "devDependencies": {
    "@types/tap": "^15.0.7",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "eslint": "^8.30.0",
    "jest": "^29.3.1",
    "prettier": "^2.8.1",
    "serve": "^14.1.2",
    "tap": "^16.3.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "parcel": "^2.8.2"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended"
    ]
  },
  "browserslist": "last 2 Chrome versions"
}
